{
   "version": "0.2.0",
   "configurations": [
         {
         "name": "Launch Bad Token",
         "type": "node",
         "request": "launch",
         "program": "${workspaceRoot}/node_modules/yo/lib/cli.js",
         "stopOnEntry": false,
         "args": [
            "tfs",
            "node",
            "nodeDemo",
            "[your vsts account]",
            "this is not a real access token",
            "Free Trial",
            "Hosted",
            "paas",
            "false"
         ],
         "cwd": "${workspaceRoot}",
         "preLaunchTask": null,
         "runtimeExecutable": null,
         "runtimeArgs": [
            "--nolazy"
         ],
         "env": {
            "NODE_ENV": "development"
         },
         "externalConsole": false,
         "sourceMaps": false,
         "outDir": "${workspaceRoot}/temp"
      },
